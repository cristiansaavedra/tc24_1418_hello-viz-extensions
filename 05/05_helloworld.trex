<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  <worksheet-extension id="com.cristiansaavedra.samples.steps.helloworld05" extension-version="1.0.1">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description>05 Hello World</description>
    <author name="Cristian Saavedra Desmoineaux" email="cristian.saavedra.desmoineaux@gmail.com" 
            organization="ComuniDatos"           website="https://medium.com/@cristiansaavedra"/>
    <min-api-version>1.1</min-api-version>
